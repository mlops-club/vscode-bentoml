{
  "compilerOptions": {
    "module": "CommonJS",
    "resolveJsonModule": true,
    "target": "ES2022",
    "lib": ["ES2022"],
    "sourceMap": true,
    "rootDir": "src",
    "strict": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "paths": {
      "@/*": ["src/*"]
    },
    "esModuleInterop": true,
    "baseUrl": ".",
    "outDir": "out"
  }
}
